<!DOCTYPE html>
<html>
  <head>
    <title>InkStream</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload', async: true %>
  </head>

  <body>
    <%= render "layouts/flash_message"%>
    <div class="container">  
      <main>
        <div class="content">
          <%= yield %>
        </div>
      </main>
    </div>
  </body>
</html>
